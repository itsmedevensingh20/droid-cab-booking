                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_42dp"
                android:layout_marginTop="@dimen/_16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_6dp"
                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/profileCV"
                app:layout_constraintStart_toStartOf="@id/profileCV"
                app:layout_constraintTop_toBottomOf="@id/moneyText">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/balanceIcon"
                        android:layout_width="@dimen/_20dp"
                        android:layout_height="@dimen/_20dp"
                        android:layout_margin="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_rupee_icon_svg" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/balanceText"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_0dp"
                        android:layout_marginStart="@dimen/_10dp"
                        android:layout_marginLeft="@dimen/_10dp"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:gravity="start|center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/balance"
                        android:textAlignment="gravity"
                        android:textColor="@color/color_7f7f7f"
                        app:layout_constraintBottom_toBottomOf="@id/balanceIcon"
                        app:layout_constraintEnd_toStartOf="@id/balance"
                        app:layout_constraintStart_toEndOf="@id/balanceIcon"
                        app:layout_constraintTop_toTopOf="@id/balanceIcon" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/balance"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6dp"
                        android:layout_marginRight="@dimen/_6dp"
                        android:gravity="end"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/rupees_symbol"
                        android:textAlignment="viewEnd"
                        android:textColor="@color/color_7f7f7f"
                        app:layout_constraintBottom_toBottomOf="@id/balanceIcon"
                        app:layout_constraintEnd_toStartOf="@id/updateText"
                        app:layout_constraintTop_toTopOf="@id/balanceIcon"
                        tools:ignore="TextContrastCheck"
                        tools:text="$ 600" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/updateText"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6dp"
                        android:layout_marginRight="@dimen/_6dp"
                        android:gravity="end"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/update"
                        android:textAlignment="viewEnd"
                        android:textColor="@color/color_f9c22d"
                        app:layout_constraintBottom_toBottomOf="@id/balanceIcon"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/balanceIcon"
                        tools:ignore="TextContrastCheck" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
