                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="@id/paymentIcon"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/paymentIcon"
                        app:srcCompat="@drawable/ic_right_arrow_svg" />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/bankCV"
                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_42dp"
                android:layout_marginTop="@dimen/_16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_6dp"
                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/paymentCV"
