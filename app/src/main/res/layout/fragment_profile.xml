                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/bankText"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_0dp"
                        android:layout_marginStart="@dimen/_10dp"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:gravity="start|center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/bank_details"
                        android:textAlignment="gravity"
                        android:textColor="@color/color_7f7f7f"
                        app:layout_constraintBottom_toBottomOf="@id/bankIcon"
                        app:layout_constraintEnd_toStartOf="@id/bankArrow"
                        app:layout_constraintStart_toEndOf="@id/bankIcon"
                        app:layout_constraintTop_toTopOf="@id/bankIcon" />


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/bankArrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="@id/bankIcon"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/bankIcon"
                        app:srcCompat="@drawable/ic_right_arrow_svg" />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/legalText"
                style="@style/Text_medium_14sp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="@string/legal"
                android:textColor="@color/black"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="@id/bankCV"
                app:layout_constraintStart_toStartOf="@id/bankCV"
                app:layout_constraintTop_toBottomOf="@id/bankCV" />


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/aboutCV"
                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_42dp"
                android:layout_marginTop="@dimen/_16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_6dp"
                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/legalText"
                app:layout_constraintStart_toStartOf="@id/legalText"
                app:layout_constraintTop_toBottomOf="@id/legalText">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/aboutIcon"
                        android:layout_width="@dimen/_20dp"
                        android:layout_height="@dimen/_20dp"
                        android:layout_margin="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_about_svg" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/aboutText"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_0dp"
                        android:layout_marginStart="@dimen/_10dp"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:gravity="start|center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/about"
                        android:textAlignment="gravity"
                        android:textColor="@color/color_7f7f7f"
                        app:layout_constraintBottom_toBottomOf="@id/aboutIcon"
                        app:layout_constraintEnd_toStartOf="@id/aboutArrow"
                        app:layout_constraintStart_toEndOf="@id/aboutIcon"
                        app:layout_constraintTop_toTopOf="@id/aboutIcon" />


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/aboutArrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="@id/aboutIcon"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/aboutIcon"
                        app:srcCompat="@drawable/ic_right_arrow_svg" />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/helpCV"
                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_42dp"
                android:layout_marginTop="@dimen/_16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_6dp"
                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/aboutCV"
                app:layout_constraintStart_toStartOf="@id/aboutCV"
                app:layout_constraintTop_toBottomOf="@id/aboutCV">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/helpIcon"
                        android:layout_width="@dimen/_20dp"
                        android:layout_height="@dimen/_20dp"
                        android:layout_margin="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_help_svg" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/helpText"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_0dp"
                        android:layout_marginStart="@dimen/_10dp"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:gravity="start|center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/help"
                        android:textAlignment="gravity"
                        android:textColor="@color/color_7f7f7f"
                        app:layout_constraintBottom_toBottomOf="@id/helpIcon"
                        app:layout_constraintEnd_toStartOf="@id/helpArrow"
                        app:layout_constraintStart_toEndOf="@id/helpIcon"
                        app:layout_constraintTop_toTopOf="@id/helpIcon" />


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/helpArrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="@id/helpIcon"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/helpIcon"
                        app:srcCompat="@drawable/ic_right_arrow_svg" />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/termsCV"
                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_42dp"
                android:layout_marginTop="@dimen/_16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_6dp"
                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/helpCV"
                app:layout_constraintStart_toStartOf="@id/helpCV"
                app:layout_constraintTop_toBottomOf="@id/helpCV">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/termsIcon"
                        android:layout_width="@dimen/_20dp"
                        android:layout_height="@dimen/_20dp"
                        android:layout_margin="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_terms_conditions_svg" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/termsText"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_0dp"
                        android:layout_marginStart="@dimen/_10dp"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:gravity="start|center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/terms_conditions"
                        android:textAlignment="gravity"
                        android:textColor="@color/color_7f7f7f"
                        app:layout_constraintBottom_toBottomOf="@id/termsIcon"
                        app:layout_constraintEnd_toStartOf="@id/termsArrow"
                        app:layout_constraintStart_toEndOf="@id/termsIcon"
                        app:layout_constraintTop_toTopOf="@id/termsIcon" />


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/termsArrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="@id/termsIcon"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/termsIcon"
                        app:srcCompat="@drawable/ic_right_arrow_svg" />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/dataCV"
                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_42dp"
                android:layout_marginTop="@dimen/_16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_6dp"
                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/termsCV"
                app:layout_constraintStart_toStartOf="@id/termsCV"
