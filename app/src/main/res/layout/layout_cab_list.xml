        <!--            app:layout_constraintEnd_toStartOf="@id/cl"-->
        <!--            app:layout_constraintStart_toEndOf="@id/carImage"-->
        <!--            app:layout_constraintTop_toTopOf="parent">-->

        <!--        </View>-->


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="@dimen/_6sdp"
            android:paddingHorizontal="@dimen/_6sdp"
            android:paddingVertical="@dimen/_4sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/carImage"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/vehicleCL1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6dp"
                android:layout_marginRight="@dimen/_6dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/vehicleCL2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleNameText"
                    style="@style/Text_regular_10sp"
                    android:layout_width="@dimen/_90dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:text="@string/vehicle_name"
                    android:textAlignment="center"
                    android:textColor="@color/color_7f7f7f"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleName"
                    style="@style/Text_medium_12sp"
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    app:layout_constraintBottom_toTopOf="@id/vehicleStatusText"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/vehicleNameText"
                    tools:text="Vehicle Name" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleStatusText"
                    style="@style/Text_regular_10sp"
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:maxLength="36"
                    android:text="@string/vehicle_status"
                    android:textAlignment="center"
                    android:textColor="@color/color_7f7f7f"
                    app:layout_constraintBottom_toTopOf="@id/vehicleStatus"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/vehicleName"
                    tools:text="Vehicle Status" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleStatus"
                    style="@style/Text_medium_12sp"
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    tools:text="Active"
                    android:textAlignment="center"
                    android:textColor="@color/green_text"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@id/vehicleStatusText"
                    app:layout_constraintStart_toStartOf="parent"
                    />


            </androidx.constraintlayout.widget.ConstraintLayout>


            <View
                android:id="@+id/view1"
                android:layout_width="@dimen/_1dp"
                android:layout_height="@dimen/_26dp"
                android:layout_marginStart="@dimen/_4dp"
                android:layout_marginTop="@dimen/_4dp"
                android:layout_marginEnd="@dimen/_4dp"
                android:layout_marginBottom="@dimen/_4dp"
                android:background="@color/color_f1f1f1"
                app:layout_constraintBottom_toTopOf="@id/view2"
                app:layout_constraintEnd_toStartOf="@id/vehicleCL2"
                app:layout_constraintStart_toEndOf="@id/vehicleCL1"
                app:layout_constraintTop_toTopOf="@id/vehicleCL1" />


            <View
                android:id="@+id/view2"
                android:layout_width="@dimen/_1dp"
                android:layout_height="@dimen/_26dp"
                android:layout_marginTop="@dimen/_4dp"
                android:layout_marginBottom="@dimen/_4dp"
                android:background="@color/color_f1f1f1"
                app:layout_constraintBottom_toBottomOf="@id/vehicleCL1"
                app:layout_constraintEnd_toEndOf="@id/view1"
                app:layout_constraintStart_toStartOf="@id/view1"
                app:layout_constraintTop_toBottomOf="@id/view1" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/vehicleCL2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_6dp"
                android:layout_marginLeft="@dimen/_6dp"
                android:layout_marginEnd="@dimen/_6dp"
                app:layout_constraintStart_toEndOf="@+id/vehicleCL1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/view3"
                app:layout_constraintTop_toTopOf="parent">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleNumberText"
                    style="@style/Text_regular_10sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLength="36"
                    android:maxLines="1"
                    android:text="@string/vehicle_number"
                    android:textAlignment="center"
                    android:textColor="@color/color_7f7f7f"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="Vehicle Number Number" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleNumber"
                    style="@style/Text_medium_12sp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="@id/vehicleNumberText"
                    app:layout_constraintStart_toStartOf="@id/vehicleNumberText"
                    app:layout_constraintTop_toBottomOf="@id/vehicleNumberText"
                    tools:text="Vehicle Number" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleRequestText"
                    style="@style/Text_regular_10sp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4dp"
                    android:gravity="center"
                    android:maxLines="1"
                    android:text="@string/vehicle_request"
                    android:textAlignment="center"
                    android:textColor="@color/color_7f7f7f"
                    app:layout_constraintBottom_toTopOf="@id/vehicleRequest"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/vehicleNumber"
                   />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleRequest"
                    style="@style/Text_medium_12sp"
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    tools:text="Active"
                    android:textAlignment="center"
                    android:textColor="@color/green_text"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@id/vehicleRequestText"
                    app:layout_constraintStart_toStartOf="@id/vehicleRequestText"
                    app:layout_constraintTop_toBottomOf="@id/vehicleRequestText"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <View
                android:id="@+id/view3"
                android:layout_width="@dimen/_1dp"
                android:layout_height="@dimen/_26dp"
                android:layout_marginStart="@dimen/_4dp"
                android:layout_marginLeft="@dimen/_4dp"
                android:layout_marginTop="@dimen/_4dp"
                android:layout_marginBottom="@dimen/_4dp"
                android:background="@color/color_f1f1f1"
                app:layout_constraintBottom_toTopOf="@id/view4"
                app:layout_constraintEnd_toStartOf="@+id/vehicleCL3"
                app:layout_constraintStart_toEndOf="@id/vehicleCL2"
                app:layout_constraintTop_toTopOf="parent" />


            <View
                android:id="@+id/view4"
                android:layout_width="@dimen/_1dp"
                android:layout_height="@dimen/_26dp"
                android:layout_marginTop="@dimen/_4dp"
                android:layout_marginBottom="@dimen/_4dp"
                android:background="@color/color_f1f1f1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/view3"
                app:layout_constraintTop_toBottomOf="@id/view3" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/vehicleCL3"
                android:layout_width="@dimen/_48dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/view3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/modelText"
                    style="@style/Text_regular_10sp"
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLength="30"
                    android:maxLines="1"
                    android:text="@string/model"
                    android:textAlignment="center"
                    android:textColor="@color/color_7f7f7f"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/model"
                    style="@style/Text_medium_12sp"
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="@+id/modelText"
                    app:layout_constraintStart_toStartOf="@id/modelText"
                    app:layout_constraintTop_toBottomOf="@id/modelText"
                    tools:text="2018" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleRatingText"
                    style="@style/Text_regular_10sp"
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:text="@string/rating"
                    android:textAlignment="center"
                    android:textColor="@color/color_7f7f7f"
                    app:layout_constraintBottom_toTopOf="@id/vehicleRating"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/model"
                    tools:text="Rating" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vehicleRating"
                    style="@style/Text_medium_12sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableStart="@drawable/ic_filled_star_svg"
                    android:drawableLeft="@drawable/ic_filled_star_svg"
                    android:drawablePadding="@dimen/_2dp"
                    android:gravity="center"
                    android:maxLines="1"
                    tools:text="+4"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@id/vehicleRatingText"
                    app:layout_constraintStart_toStartOf="@id/vehicleRatingText"
                    app:layout_constraintTop_toBottomOf="@id/vehicleRatingText" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>