                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/balanceCV"
                app:layout_constraintStart_toStartOf="@id/balanceCV"
                app:layout_constraintTop_toBottomOf="@id/balanceCV">

                <androidx.constraintlayout.widget.ConstraintLayout
