            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ratingText"
                style="@style/Text_medium_14sp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="@string/ratings"
                android:textColor="@color/black"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="@id/dataCV"
                app:layout_constraintStart_toStartOf="@id/dataCV"
                app:layout_constraintTop_toBottomOf="@id/dataCV" />

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/ratingCV"
                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_42dp"
                android:layout_marginTop="@dimen/_16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_6dp"
                app:cardElevation="@dimen/_6dp"
                app:layout_constraintEnd_toEndOf="@id/ratingText"
                app:layout_constraintStart_toStartOf="@id/ratingText"
                app:layout_constraintTop_toBottomOf="@id/ratingText">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ratingIcon"
                        android:layout_width="@dimen/_20dp"
                        android:layout_height="@dimen/_20dp"
                        android:layout_margin="@dimen/_10dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_ratings_svg" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/ratingShowText"
                        style="@style/Text_medium_14sp"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_0dp"
                        android:layout_marginStart="@dimen/_10dp"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:gravity="start|center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/ratings"
                        android:textAlignment="gravity"
                        android:textColor="@color/color_7f7f7f"
                        app:layout_constraintBottom_toBottomOf="@id/ratingIcon"
                        app:layout_constraintEnd_toStartOf="@id/ratingArrow"
                        app:layout_constraintStart_toEndOf="@id/ratingIcon"
                        app:layout_constraintTop_toTopOf="@id/ratingIcon" />


                    <RatingBar
                        android:id="@+id/ratingArrow"
                        style="@style/Custom_RatingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:numStars="5"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>


            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/logoutBtn"
                android:layout_width="@dimen/_0dp"
                android:layout_height="@dimen/_50dp"
                android:layout_marginTop="@dimen/_30dp"
                android:layout_marginBottom="@dimen/_14dp"
                android:background="@drawable/bg_pink_red_outline_2dp"
                android:onClick="@{(v)->profileViewModel.onClick(v)}"
                android:text="@string/title_logout"
                android:textAllCaps="false"
                android:textColor="@color/color_fe2929"
                android:textSize="@dimen/_16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@id/ratingCV"
                app:layout_constraintStart_toStartOf="@id/ratingCV"
                app:layout_constraintTop_toBottomOf="@id/ratingCV" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

</layout>